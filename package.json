{
  "name": "webstart-project-id",
  "version": "1.0.0",
  "description": "webstart-project-name",
  "author": "webstart-project-author",
  "license": "UNLICENSED",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon -r tsconfig-paths/register src/index.ts",
    "start:production": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register src/index.ts",
    "production:build": "npm run clean-build-dirs && tsc --project tsconfig.production.json",
    "production:start": "cross-env NODE_ENV=production TS_NODE_BASEURL=./dist node -r tsconfig-paths/register dist/index.js",
    "clean-build-dirs": "rimraf dist .tsbuildinfo",
    "test": "jest",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.+(js|ts)\""
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.1",
    "@types/morgan": "1.9.4",
    "@types/node": "20.1.7",
    "@types/supertest": "2.0.12", 
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "rimraf": "3.0.2",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "commander": "10.0.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "express": "4.18.2",
    "helmet": "7.0.0",
    "morgan": "1.10.0",
    "tsconfig-paths": "4.2.0",
    "winston": "3.8.2"
  }
}